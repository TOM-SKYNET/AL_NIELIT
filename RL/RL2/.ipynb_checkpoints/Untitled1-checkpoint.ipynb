    "import gym\n",
    "import numpy as np\n",
    "\n",
    "env = gym.make(\"Taxi-v2\")\n",
    "state = env.reset()\n",
    "env.render()\n",
    "\n",
    "count = 1\n",
    "while count < 5:\n",
    "    action = env.action_space.sample()\n",
    "    state,reward,done,info = env.step(action)\n",
    "    count = count + 1\n",
    "    \n",
    "print \"No of steps for final state:\",count\n"
   ]
